                    <colspec colname="newCol3" colnum="3" colwidth="1.14*"/>
                    <colspec colname="c2" colnum="4" colwidth="6.56*"/>
                    <colspec colname="newCol5" colnum="5" colwidth="9.14*"/>
                    <colspec colname="newCol6" colnum="6" colwidth="7.84*"/>
                    <tbody>
                        <row>
                            <entry><image href="../Images/I_enterprise_bullet_type.png"
                                    id="image_epg_zmw_vxb"/></entry>
                            <entry namest="newCol2" nameend="newCol6">
                                <p><b>Enterprise</b></p>
                            </entry>
                        </row>
                        <row>
                            <entry/>
